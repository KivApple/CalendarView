<?xml version="1.0" encoding="utf-8"?>
<resources>
	<attr name="todayColor" format="color" />
	<attr name="regularColor" format="color" />
	<attr name="activeColor" format="color" />
	
	<declare-styleable name="TextGridView">
		<attr name="textSize" format="dimension" />
		<attr name="cellSize" format="dimension" />
		<attr name="cellMargin" format="dimension" />
		<attr name="borderWidth" format="dimension" />
		<attr name="headerHeight" format="dimension" />
	</declare-styleable>
	<declare-styleable name="WeekView">
		<attr name="highlightToday" format="boolean" />
		<attr name="todayColor" />
		<attr name="regularColor" />
		<attr name="activeColor" />
	</declare-styleable>
	<declare-styleable name="MonthView">
		<attr name="todayColor" />
		<attr name="regularColor" />
		<attr name="activeColor" />
		<attr name="inactiveColor" format="color" />
	</declare-styleable>
</resources>
